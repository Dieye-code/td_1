import java.util.Scanner;

public class Exo10 {

	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter la valeur de a");
		int a = sc.nextInt();
		System.out.println("Enter la valeur de b");
		int b = sc.nextInt();
		System.out.println("Enter la valeur de c");
		int c = sc.nextInt();
		
		int d =0;
		
		if(a>b) {
			d=a;
			a=b;
			b=d;
		}
		if(a>c) {
			d=a;
			a=c;
			c=d;
		}
		
		System.out.println("L'ordre croissant est "+a+" - "+b+" - "+c);
		
		sc.close();

	}
	
	
}
