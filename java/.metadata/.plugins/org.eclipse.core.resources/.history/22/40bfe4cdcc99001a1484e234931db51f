package exo21;

import java.util.Scanner;

public class Exo21 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Entrer le nombre à deviner");
		int n = sc.nextInt();
		int x =0;
		
		do {
			int x = sc.nextInt();
		} while (x!=n);
		
		sc.close();

	}

}
